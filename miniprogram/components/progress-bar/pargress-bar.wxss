/* components/progress-bar/pargress-bar.wxss */
.container{
  display: flex;
  align-items: center;
}
.control{
  flex: 1;
  position: relative;
}
.movable-area{
  width: 100%;
  height: 34rpx;
  position: absolute;
  bottom: -14rpx;
  left: 0;

}
.movable-view{
  width: 36rpx;
  height: 36rpx;
  background-color: #ff6666;
  border-radius: 50%;
}

.time{
width: 64rpx;
padding:  0 20rpx;
font-size: 24rpx;
color: rgb(255, 255, 255);
font-weight: 499;
line-height: 30rpx;
}